{
  "name": "losant-cli",
  "version": "1.0.0",
  "bin": {
    "losant": "./bin/losant.js",
    "losant-configure": "./bin/losant-configure.js",
    "losant-views": "./bin/losant-views.js",
    "losant-files": "./bin/losant-files.js"
  },
  "engines": {
    "node": "8.12.0",
    "yarn": "^1.9.4"
  },
  "scripts": {
    "lint": "esw . --ext .js",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "test": "mocha ./test",
    "reinstall": "rm -rf nodes_modules && yarn install"
  },
  "dependencies": {
    "@rjhilgefort/export-dir": "^1.1.0",
    "chalk": "^2.3.0",
    "commander": "^2.11.0",
    "form-data": "^2.3.2",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.2",
    "inquirer": "^3.3.0",
    "js-yaml": "^3.10.0",
    "lodash-template": "^1.0.0",
    "losant-rest": "^2.1.0",
    "mime-types": "^2.1.18",
    "minimatch": "^3.0.4",
    "mkdirp": "^0.5.1",
    "omnibelt": "^1.2.0",
    "opener": "^1.4.3",
    "pad": "^2.0.3",
    "request": "^2.83.0",
    "sanitize-filename": "^1.6.1",
    "single-line-log": "^1.1.2",
    "sync-request": "^6.0.0",
    "update-notifier": "^2.3.0"
  },
  "devDependencies": {
    "@losant/eslint-config-losant": "^1.3.4",
    "mocha": "^5.2.0",
    "nock": "^9.6.1",
    "rimraf": "^2.6.2",
    "should": "^13.2.3",
    "sinon": "^6.1.5"
  },
  "eslintConfig": {
    "extends": "@losant/eslint-config-losant/env/node",
    "rules": {
      "no-shadow": "error"
    }
  }
}
